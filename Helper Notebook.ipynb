   "source": [
    "import glob\n",
    "from PIL import Image \n",
    "webp_files = glob.glob('images/*.webp')\n",
    "\n",
    "for file in webp_files:\n",
    "    img = Image.open(file)\n",
    "    img.save(file.replace('.webp', '.png'), 'png')\n",
    "    print(f'Converting {file} to {file.replace(\".webp\", \".png\")}')\n"
   ]
